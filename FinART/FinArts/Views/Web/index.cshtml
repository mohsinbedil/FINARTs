					</div>
				</div>	
			</section>
			<!-- End quote Area -->

			<!-- Start exibition Area -->
			<section class="exibition-area section-gap" id="exhibitions">
				<div class="container">
					<div class="row d-flex justify-content-center">
						<div class="menu-content pb-60 col-lg-10">
							<div class="title text-center">
								<h1 class="mb-10">Ongoing Exhibitions from the scratch</h1>
								<p>Who are in extremely love with eco friendly system.</p>
							</div>
						</div>
					</div>						
					<div class="row">
						<div class="active-exibition-carusel">
							@foreach(var items in Model.Exhibitions){
							<div class="single-exibition item">
								<img src="~/Content/Images/@items.ExhibitionIMG" alt="">					
								<h4>@items.Name Expo</h4>								
								<h6 class="date">Starting From  @items.StartDate </h6>
								<h6 class="date">Ending At  @items.EndDate </h6>
						<a href="#" class="primary-btn text-uppercase">View Details</a>

							</div>}

							
						</div>													
					</div>
				</div>	
			</section>
			<!-- End exibition Area -->			
		
			<!-- Start upcoming-event Area -->
			<section class="upcoming-event-area section-gap" id="events">
				<div class="container">
					<div class="row d-flex justify-content-center">
						<div class="menu-content pb-60 col-lg-10">
							<div class="title text-center">
								<h1 class="mb-10">Checkout our Upcoming Competitions</h1>
								<p>Who are in extremely love with eco friendly system.</p>
							</div>
						</div>
					</div>
		<div class="row">
			@foreach(var item in Model.Competitions){
						<div class="col-lg-6 event-left">
							<div class="single-events">
								<img class="img-fluid" src="~/Content/Images/@item.IMG" alt="" >
							
								<h4>Event @item.Name</h4>
								<h5>
								Award (@item.AwardDetails) 
								</h5>
						<h6 class="date">Starting From  @item.StartDate </h6>
						<h6 class="date">Ending At  @item.EndDate </h6>
								<a asp-controller="Submissions" asp-action="Create" class="primary-btn text-uppercase">Submit your Designs</a>
							</div>
											
						</div>}
			
			
					</div>
				</div>	
			</section>
			<!-- End upcoming-event Area -->
		

			<!-- Start gallery Area -->
			<section class="gallery-area section-gap" id="gallery">
				<div class="container">
					<div class="row d-flex justify-content-center">
						<div class="menu-content pb-70 col-lg-8">
							<div class="title text-center">
								<h1 class="mb-10 text-white">Our Competition Gallery</h1>
								<p>Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore  et dolore magna aliqua.</p>
							</div>
						</div>
					</div>						
					<div id="grid-container" class="row">
						<a class="single-gallery" href="img/g1.jpg"><img class="grid-item" src="img/g1.jpg"></a>
						<a class="single-gallery" href="img/g2.jpg"><img class="grid-item" src="img/g2.jpg"></a>
						<a class="single-gallery" href="img/g3.jpg"><img class="grid-item" src="img/g3.jpg"></a>
						<a class="single-gallery" href="img/g4.jpg"><img class="grid-item" src="img/g4.jpg"></a>
						<a class="single-gallery" href="img/g5.jpg"><img class="grid-item" src="img/g5.jpg"></a>
						<a class="single-gallery" href="img/g6.jpg"><img class="grid-item" src="img/g6.jpg"></a>
						<a class="single-gallery" href="img/g7.jpg"><img class="grid-item" src="img/g7.jpg"></a>
						<a class="single-gallery" href="img/g8.jpg"><img class="grid-item" src="img/g8.jpg"></a>
						<a class="single-gallery" href="img/g9.jpg"><img class="grid-item" src="img/g9.jpg"></a>						
						@foreach(var item in Model.Submissions){
						<a class="single-gallery" href="img/g5.jpg"><img class="grid-item" src="~/Content/Images/@item.DIMG"></a>
						}
					</div>	
				</div>	
			</section>
			<!-- End gallery Area -->
			

			



